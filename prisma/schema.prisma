generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GoogleAuthRefreshToken {
  id           Int    @id @default(autoincrement())
  refreshToken String @map("refresh_token")

  @@map("Google_Auth_Refresh_Token")
}

model Order {
  id                  Int    @id @default(autoincrement())
  orderOnMarketPlace  String @map("order_on_market_place")
  jsonb               Json

  @@map("order")
}

model LogisticsShippedOrders {
  id                        Int       @id @default(autoincrement())
  sku                       String    @map("sku")
  orderOnMarketPlace        String    @map("order_on_market_place")
  status                    String    @default("pending") @map("status")
  uploads                   String[]  @map("uploads")
  ordersJsonb               Json      @map("orders_jsonb")
  rateQuotesResponseJsonb   Json      @map("rate_quotes_response_jsonb")
  bolResponseJsonb          Json      @map("bol_response_jsonb")
  pickupResponseJsonb       Json      @map("pickup_response_jsonb")
  createdAt                 DateTime  @default(now()) @map("created_at")
  updatedAt                 DateTime  @updatedAt @map("updated_at")

  @@map("Logistics_Shippped_Orders")
}

model ShippingCompanyToken {
  id                  Int     @id @default(autoincrement())
  shippingCompanyName String  @unique @map("shipping_company_name")
  token               String  @map("token")
  createdAt           DateTime @default(now()) @map("created_at")
  updatedAt           DateTime @updatedAt @map("updated_at")

  @@map("shipping_company_token")
}

