generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GoogleAuthRefreshToken {
  id           Int    @id @default(autoincrement())
  refreshToken String @map("refresh_token")

  @@map("Google_Auth_Refresh_Token")
}

model Order {
  id                  Int    @id @default(autoincrement())
  orderOnMarketPlace  String @map("order_on_market_place")
  jsonb               Json

  @@map("order")
}

